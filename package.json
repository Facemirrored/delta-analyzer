{
  "name": "delta-ads-analyzer",
  "displayName": "DELTA-ADS-ANALYZER",
  "description": "DELTA ADS language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "delta-ads",
      "aliases": ["delta-ads"],
      "extensions": [".cob", ".cbl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "delta-ads",
      "scopeName": "source.delta-ads",
      "path": "./syntaxes/delta.tmLanguage.json"
    }]
  }
}
